{
    "name": "changelog/formatter-keep-a-changelog",
    "description": "TBD",
    "keywords": [
        "changelog",
        "formatter",
        "git"
    ],
    "homepage": "https://github.com/changelog-page/formatter-keep-a-changelog",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "changelog/formatter": "^0.1.0",
        "illuminate/support": "^9.0",
        "nesbot/carbon": "^2.56"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "Changelog\\Formatter\\KeepAChangelog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Changelog\\Formatter\\KeepAChangelog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
